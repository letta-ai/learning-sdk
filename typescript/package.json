{
  "name": "@letta-ai/agentic-learning",
  "version": "0.2.2",
  "description": "Agentic Learning SDK for TypeScript - Automatic memory integration with Letta",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:openai": "jest openai.test.ts",
    "test:anthropic": "jest anthropic.test.ts",
    "test:gemini": "jest gemini.test.ts",
    "test:claude": "jest claude.test.ts",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "example:openai": "cd ../examples && NODE_PATH=../typescript/node_modules ts-node openai_example.ts",
    "example:anthropic": "cd ../examples && NODE_PATH=../typescript/node_modules ts-node anthropic_example.ts",
    "example:gemini": "cd ../examples && NODE_PATH=../typescript/node_modules ts-node gemini_example.ts",
    "example:claude": "cd ../examples && NODE_PATH=../typescript/node_modules ts-node claude_example.ts"
  },
  "keywords": [
    "letta",
    "memory",
    "learning",
    "ai",
    "llm",
    "openai",
    "anthropic",
    "gemini",
    "claude"
  ],
  "author": "Letta",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/letta-ai/agentic-learning-sdk.git",
    "directory": "typescript"
  },
  "homepage": "https://github.com/letta-ai/agentic-learning-sdk#readme",
  "bugs": {
    "url": "https://github.com/letta-ai/agentic-learning-sdk/issues"
  },
  "files": [
    "dist",
    "README.md",
    "../LICENSE"
  ],
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "@anthropic-ai/sdk": ">=0.30.1",
    "@google/generative-ai": ">=0.21.0",
    "openai": ">=4.104.0"
  },
  "peerDependenciesMeta": {
    "openai": {
      "optional": true
    },
    "@anthropic-ai/sdk": {
      "optional": true
    },
    "@google/generative-ai": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@letta-ai/letta-client": "1.0.0-alpha.18",
    "ts-node": "^10.9.2"
  }
}
