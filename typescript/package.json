{
  "name": "@letta-ai/agentic-learning",
  "version": "0.4.3",
  "description": "Drop-in SDK to add an AI memory layer to any application. Works with OpenAI, Anthropic, Gemini, Claude, Vercel AI SDK.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:openai": "jest openai.test.ts",
    "test:anthropic": "jest anthropic.test.ts",
    "test:gemini": "jest gemini.test.ts",
    "test:claude": "npm run build && npx tsx tests/integration/claude.test.mts",
    "test:integration": "npm test -- tests/integration/ && npm run test:claude",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "example:openai": "cd ../examples && NODE_PATH=../typescript/node_modules ts-node openai_example.ts",
    "example:anthropic": "cd ../examples && NODE_PATH=../typescript/node_modules ts-node anthropic_example.ts",
    "example:gemini": "cd ../examples && NODE_PATH=../typescript/node_modules ts-node gemini_example.ts",
    "example:claude": "cd ../examples && NODE_PATH=../typescript/node_modules ts-node claude_example.ts"
  },
  "keywords": [
    "ai-memory",
    "llm-memory",
    "agent-memory",
    "persistent-memory",
    "continual-learning",
    "ai-agents",
    "llm",
    "openai",
    "anthropic",
    "claude",
    "gemini",
    "vercel-ai",
    "letta",
    "memory-layer",
    "stateful-agents",
    "conversational-ai",
    "rag",
    "context-management",
    "semantic-memory",
    "long-term-memory",
    "chatbot-memory",
    "ai-context"
  ],
  "author": "Caren Thomas",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/letta-ai/agentic-learning-sdk.git",
    "directory": "typescript"
  },
  "homepage": "https://github.com/letta-ai/agentic-learning-sdk#readme",
  "bugs": {
    "url": "https://github.com/letta-ai/agentic-learning-sdk/issues"
  },
  "files": [
    "dist",
    "README.md",
    "../LICENSE"
  ],
  "devDependencies": {
    "@ai-sdk/anthropic": "^2.0.44",
    "@ai-sdk/openai": "^2.0.65",
    "@anthropic-ai/claude-agent-sdk": "^0.1.37",
    "@anthropic-ai/sdk": "^0.68.0",
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@google/generative-ai": "^0.24.1",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "ai": "^5.0.93",
    "babel-jest": "^30.2.0",
    "eslint": "^8.0.0",
    "jest": "^29.5.0",
    "openai": "^6.8.1",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "@anthropic-ai/sdk": ">=0.30.1",
    "@google/generative-ai": ">=0.21.0",
    "openai": ">=4.104.0"
  },
  "peerDependenciesMeta": {
    "openai": {
      "optional": true
    },
    "@anthropic-ai/sdk": {
      "optional": true
    },
    "@google/generative-ai": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@letta-ai/letta-client": "^1.0.0-alpha.20",
    "ts-node": "^10.9.2"
  }
}
